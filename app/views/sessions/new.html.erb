<h1>Log in</h1>
<%= form_with url: session_path do |form| %>
  <div class='field'>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>

  <div class='field'>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div class='actions'>
    <%= form.submit 'Log In' %>
  </div>
<% end %>
